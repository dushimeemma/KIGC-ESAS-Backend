{
    "root": true,
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "env": {
        "node": true,
        "es6": true,
        "mocha": true
    },
    "rules": {
        "one-var": 0,
        "one-var-declaration-per-line": 0,
        "new-cap": 0,
        "consistent-return": 0,
        "no-param-reassign": 0,
        "comma-dangle": 0,
        "no-underscore-dangle": 0,
        "no-console": 0,
        "class-methods-use-this": 0,
        "import/prefer-default-export": 0
    }
}