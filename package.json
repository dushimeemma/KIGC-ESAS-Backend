{
  "name": "kigc-esas-backend",
  "version": "1.0.0",
  "description": "exam seats arrangement system backend",
  "main": "index.js",
  "scripts": {
    "prestart": "npx sequelize db:migrate",
    "start": "npm run prestart && node --require babel-register index.js",
    "dev": "NODE_ENV=development npm run prestart && nodemon --require babel-register index.js",
    "test": "node_modules/.bin/sequelize db:migrate:undo:all --env test && node_modules/.bin/sequelize  db:migrate --env test && NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 10000 --require babel-register tests/* --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dushimeemma/KIGC-ESAS-Backend.git"
  },
  "author": "Emma Dushime",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dushimeemma/KIGC-ESAS-Backend/issues"
  },
  "homepage": "https://github.com/dushimeemma/KIGC-ESAS-Backend#readme",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.4.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0"
  }
}
